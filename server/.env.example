# HTTP/WebSocket server
WS_PORT=9885

# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5.2
OPENAI_REASONING_EFFORT=high
OPENAI_REASONING_EFFORT_BATTLE=high
OPENAI_REASONING_EFFORT_DIALOG=high
OPENAI_REASONING_EFFORT_CRITICISM=high
OPENAI_REASONING_EFFORT_SUMMARY=xhigh
OPENAI_MODEL_PATHFINDING=gpt-5.2
